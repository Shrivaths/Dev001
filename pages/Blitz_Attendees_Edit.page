                            <label>Level Completion</label>       
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Stage__c}" required="false" styleclass="form-control"/>            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <label>Blitz Plan</label>            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Blitz_Plan__c}" required="false" />           
                        </div>
                    </div>
            
