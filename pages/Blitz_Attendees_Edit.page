                            <label>Round 2 Panelist</label>       
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Round_2_Panelist__c}" required="false" />            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <label>Round 3 Panelist</label>            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Round_3_Panelist__c}" required="false" />           
                        </div>
                    </div>
                    
