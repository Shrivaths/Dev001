                            <label>Panelist HR</label>       
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Panelist_HR__c}" required="false"/>            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <label>Arrival Sequence</label>            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Arrival_Sequence__c}" required="false" styleclass="form-control"/>          
                        </div>
                    </div>
        
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 top-line-spacing">
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <label>Capability</label>       
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            <apex:inputField value="{!Blitz_Attendees__c.Capability__c}" required="false" styleclass="form-control"/>            
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                                       
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                            
                        </div>
                    </div>
                </div>
        
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 top-line-spacing btn-panel">
                    <div class="col-xs-12 col-sm-4 col-md-5 col-lg-5" style="text-align: right;">
                        <apex:commandButton value="Save" action="{!save}"/>
                    </div>
                    <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2" style="text-align: center;">
                        <apex:commandButton value="Save & New" action="{!save}" />
                    </div>
                    
                    <div class="col-xs-12 col-sm-4 col-md-5 col-lg-5" style="text-align: left;">
                        <apex:commandButton value="Cancel" action="{!cancel}"/>
                    </div>
                </div>
            
            </div>
